{
  "name": "factory-ts",
  "version": "0.5.2-mfc-1",
  "author": "Will Pleasant-Ryan + @davidvmckay",
  "license": "MIT",
  "description": "A Typescript test data factory similar to factory_bot and rosie",
  "repository": "https://github.com/masterpiece-flower-company/factory.ts.git",
  "main": "lib/index.js",
  "typings": "lib/index",
  "files": [ "lib/" ],
  "scripts": {
    "build": "tsc -p .",
    "test": "jest --colors",
    "test:debug": "node --inspect=9229 --inspect-brk ./node_modules/.bin/jest --runInBand",
    "test:watch": "yarn test:truncate && jest --watch",
    "build-and-publish": "yarn build && npm login && npm publish",
    "postinstall": "yarn build"
  },
  "jest": {
    "reporters": [ "default" ],
    "transform": { "\\.(js|ts)$": "ts-jest" },
    "testRegex": "./spec/.*\\.spec\\.ts$",
    "moduleFileExtensions": [ "js", "ts", "json" ],
    "moduleDirectories": [ "src", "node_modules" ],
    "setupFilesAfterEnv": [ "<rootDir>/jest-setup.ts" ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^10.17.0",
    "eslint": "^7.32.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.5.2",
    "jest": "^27.2.4",
    "jest-environment-node-debug": "^2.0.0",
    "ts-jest": "^27.0.5",
    "ts-node": "^9",
    "typescript": "4.4.3"
  },
  "dependencies": {
    "clone-deep": "^4.0.1",
    "source-map-support": "^0.5.20"
  }
}
